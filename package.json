{
  "private": true,
  "name": "wasm-postflop",
  "version": "0.0.1",
  "description": "Post-flop solver powered by WebAssembly",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "build": "webpack",
    "serve": "node server.js",
    "lint": "eslint src/**/*.{ts,vue}",
    "format": "prettier --write src/**/*.{ts,vue}",
    "wasm": "run-s range solver-st solver-mt solver-patch",
    "range": "rustup run nightly wasm-pack build --out-dir ../../pkg/range rust/range",
    "solver-st": "rustup run nightly wasm-pack build --target web --out-dir ../../pkg/solver-st rust/solver-st",
    "solver-mt": "rustup run nightly wasm-pack build --target web --out-dir ../../pkg/solver-mt rust/solver-mt",
    "solver-patch": "sed -i \"s/cachegetInt32Memory0 = new Int32Array(wasm.memory.buffer)/cachegetInt32Memory0 = new Uint32Array(wasm.memory.buffer)/\" ./pkg/solver-mt/solver.js ./pkg/solver-st/solver.js"
  },
  "dependencies": {
    "comlink": "^4.3.1",
    "detect-browser": "^5.3.0",
    "pinia": "^2.0.14",
    "vue": "^3.2.36"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.3",
    "@tailwindcss/forms": "^0.5.2",
    "@vue/compiler-sfc": "^3.2.36",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "autoprefixer": "^10.4.7",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "cssnano": "^5.1.10",
    "eslint": "^8.16.0",
    "eslint-plugin-vue": "^8.7.1",
    "express": "^4.18.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.0",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.0.24",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.2",
    "url-loader": "^4.1.1",
    "vue-loader": "^17.0.0",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  }
}
