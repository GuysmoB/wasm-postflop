{
  "private": true,
  "name": "wasm-postflop",
  "version": "0.0.1",
  "description": "Post-flop solver powered by WebAssembly",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "build": "webpack",
    "serve": "node server.js",
    "lint": "eslint src/**/*.{ts,vue}",
    "format": "prettier --write src/**/*.{ts,vue}",
    "wasm": "run-s wasm:no-simd wasm:simd",
    "wasm:no-simd": "cross-env RUSTFLAGS=\"-C target-feature=+atomics,+bulk-memory,+mutable-globals -C link-arg=--max-memory=4294967296\" rustup run nightly wasm-pack build --target web",
    "wasm:simd": "cross-env RUSTFLAGS=\"-C target-feature=+atomics,+bulk-memory,+mutable-globals,+simd128 -C link-arg=--max-memory=4294967296\" rustup run nightly wasm-pack build --target web --out-dir pkg-simd"
  },
  "dependencies": {
    "comlink": "^4.3.1",
    "vue": "^3.2.31",
    "wasm-feature-detect": "^1.2.11"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-plugin-vue": "^8.4.1",
    "express": "^4.17.3",
    "html-webpack-plugin": "^5.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "vue-loader": "^17.0.0",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  }
}
